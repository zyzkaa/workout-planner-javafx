<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.example.projekt.controller.StartController"
            prefHeight="400.0" prefWidth="600.0">
    <VBox spacing="15.0" alignment="TOP_CENTER" AnchorPane.topAnchor="20.0" AnchorPane.leftAnchor="20.0" AnchorPane.rightAnchor="20.0">
        <Label fx:id="welcomeLabel" text="Witaj, użytkowniku!" style="-fx-font-size: 20px; -fx-font-weight: bold;" />

        <Button fx:id="logoutButton" text="Wyloguj się" />

        <Label text="To jest strona główna aplikacji." style="-fx-text-fill: #666;" />

        <!-- Miejsce na dalszą zawartość -->
        <Pane fx:id="contentPane" prefHeight="200.0" />
    </VBox>
</AnchorPane>
